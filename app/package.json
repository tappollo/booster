    "patch-package": "^6.1.2",
    "postinstall-postinstall": "^2.0.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "3.5.2"
  }
