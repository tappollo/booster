    "@types/react-native-material-textfield": "^0.12.3",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-navigation": "^3.0.7",
    "@types/styled-components": "^4.1.16",
    "date-fns": "^2.0.0-beta.2",
    "fuse.js": "^3.4.5",
    "ramda": "^0.26.1",
    "react": "16.8.6",
    "react-native": "0.59.9",
    "react-native-code-push": "5.7.0",
