    "react-native-progress-display": "npm:@zhigang1992/react-native-progress-display@latest",
    "react-native-reanimated": "^1.1.0",
    "react-native-screens": "1.0.0-alpha.22",
    "react-native-vector-icons": "^6.5.0",
